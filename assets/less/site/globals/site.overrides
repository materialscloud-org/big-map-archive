}

/* For web browsers with max width of 643px: */
.outer-navbar {
    background-size: cover;
    background-position: center center;
}

/* For web browsers with min width of 644px: */
@media only screen and (min-width: 644px) {
    .outer-navbar {
        background-size: contain; // background image will scale, will try to fit the content area, but will keep its aspect ratio
        background-position: center center;
    }
}

